{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "capitainewp/toolbar",
	"version": "1.0",
	"title": "Toolbar",
	"category": "capitainewp",
	"icon": "bell",
	"description": "Un bloc d’alerte avec plusieurs styles.",
	"keywords": ["attention", "alerte", "conseil", "éviter", "succès"],
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p"

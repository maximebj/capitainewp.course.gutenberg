{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "capitainewp/supports",
	"version": "1.0",
	"title": "Supports",
	"category": "capitainewp",
	"icon": "art",
	"description": "Ajouter des fonctionnalités supplémentaires à un bloc.",
	"supports": {
		"align": true,
		"color": {
			"background": true,
			"text": true,
			"link": true,
			"button": true,
			"heading": true,
			"gradients": true
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"blockGap": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": true
		},
		"background": {
			"backgroundImage": true,
			"backgroundSize": true
		},
		"dimensions": {
			"minHeight": true
		},
		"position": {
			"sticky": true
		},
		"shadow": true,
		"anchor": true,
		"customClassName": true,
		"className": true,
		"html": false,
		"multiple": true,
		"renaming": true,
		"reusable": false
	},
	"example": {},
	"textdomain": "capitainewp-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
